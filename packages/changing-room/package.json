  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.2.3",
    "bootstrap": "^5.3.0-alpha1",
    "pinia": "^2.0.28",
    "pino-pretty": "^10.0.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/bootstrap": "^5.2.6",
    "@types/jsdom": "^21.1.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^21.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.57.1",
    "vite": "^4.0.0",
    "vitest": "^0.29.7",
    "vue-tsc": "^1.0.12"
  }
